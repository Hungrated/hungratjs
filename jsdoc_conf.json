{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": [
      "jsdoc",
      "closure"
    ]
  },
  "source": {
    "include": [
      "src/"
    ],
    "exclude": [
      "src/entry.js"
    ],
    "includePattern": ".+\\.js(doc)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true,
    "default": {
      "outputSourceFiles": true
    },
    "applicationName": "js-common-tools",
    "disqus": "",
    "googleAnalytics": "",
    "openGraph": {
      "title": "",
      "type": "website",
      "image": "",
      "site_name": "",
      "url": ""
    },
    "meta": {
      "title": "Documentation - js-common-tools",
      "description": "",
      "keyword": ""
    },
    "linenums": true
  },
  "markdown": {
    "parser": "gfm",
    "hardwrap": true,
    "tags": [
      "examples"
    ]
  }
}
